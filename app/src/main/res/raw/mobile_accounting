DROP TABLE IF EXISTS "invoices";
CREATE TABLE invoices (invoice_id integer PRIMARY KEY AUTOINCREMENT NOT NULL, invoice_code varchar (50), number varchar (50), provider_id integer, date_d date, date_doc date, number_doc varchar (50), invoice_type_id integer, created integer DEFAULT 0);
