DROP TABLE IF EXISTS "barcode_templates";
CREATE TABLE barcode_templates (   barcode_template_id  integer PRIMARY KEY AUTOINCREMENT NOT NULL,   type                 integer,   template             varchar(20) );
INSERT INTO "barcode_templates" VALUES(25,1,'23TTTTTMMMMMP');
INSERT INTO "barcode_templates" VALUES(26,3,'21TTTTTTTTTTP');
DROP TABLE IF EXISTS "inventories";
CREATE TABLE inventories (   inventory_id  integer PRIMARY KEY AUTOINCREMENT NOT NULL,   inventory_code  varchar(50),   number        varchar(50),   store_id      integer,   date_d        integer,   created       integer );
